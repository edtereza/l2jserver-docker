{
	"name" : "Cardio",
	"description" : "Cardio web app for Reebok Sports Club",
	"type" : "pwa",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"reebok",
		"app.pwa",
		"cardio"
	],
	"homepage" : "https://cardio.reebokclub.com.br",
	"minimum-stability" : "stable",
	"prefer-stable" : true,
	"require" : {
		"php" : "^5.6 || ^7.0",
		"zendframework/zend-component-installer" : "^1.0 || ^0.7 || ^1.0.0-dev@dev",
		"zendframework/zend-mvc" : "^3.0.1",
		"zfcampus/zf-development-mode" : "^3.0",
		"zendframework/zend-json" : "~3.1",
		"zendframework/zend-db" : "~2.9",
		"zendframework/zend-math" : "~3.1",
		"zendframework/zend-mvc-i18n" : "~1.1",
		"zendframework/zend-mvc-plugin-fileprg" : "~1.1",
		"zendframework/zend-i18n-resources" : "~2.6",
		"zendframework/zend-session" : "~2.8",
		"zendframework/zend-i18n" : "~2.9",
		"zendframework/zend-mvc-console" : "~1.2",
		"zendframework/zend-filter" : "~2.8",
		"zendframework/zend-mvc-plugin-identity" : "~1.1",
		"zendframework/zend-mvc-plugins" : "~1.0",
		"zendframework/zend-servicemanager-di" : "~1.2",
		"zendframework/zend-mvc-plugin-flashmessenger" : "~1.1",
		"zendframework/zend-psr7bridge" : "~1.0",
		"zendframework/zend-mvc-form" : "~1.0",
		"zendframework/zend-cache" : "~2.8",
		"zendframework/zend-log" : "~2.10",
		"mobiledetect/mobiledetectlib" : "~2.8",
		"phpmailer/phpmailer" : "~6.0",
		"cbschuld/browser.php" : "~1.9",
		"guzzlehttp/guzzle" : "~6.5"
	},
	"autoload" : {
		"psr-4" : {
			"Application\\" : "module/Application/src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"ApplicationTest\\" : "module/Application/test/"
		}
	},
	"scripts" : {
		"cs-check" : "phpcs",
		"cs-fix" : "phpcbf",
		"development-disable" : "zf-development-mode disable",
		"development-enable" : "zf-development-mode enable",
		"development-status" : "zf-development-mode status",
		"post-create-project-cmd" : "@development-enable",
		"serve" : "php -S 0.0.0.0:8080 -t public public/index.php",
		"test" : "phpunit"
	},
	"authors" : [{
			"name" : "Erik D. Tereza",
			"email" : "erik.tereza@edtereza.com.br",
			"role" : "Desenvolvedor"
		}
	],
	"support" : {
		"email" : "suporte@reebokclub.com.br"
	}
}